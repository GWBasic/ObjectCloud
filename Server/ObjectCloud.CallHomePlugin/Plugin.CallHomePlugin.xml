<?xml version="1.0" encoding="utf-8" ?>

<objects xmlns="http://www.springframework.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.net, http://www.springframework.net/xsd/spring-objects-1.1.xsd">

  <object id="CallHomePlugin" type="ObjectCloud.CallHomePlugin.CallHomePlugin, ObjectCloud.CallHomePlugin">
    <property name="FileHandlerFactoryLocator" ref="FileHandlerFactoryLocator" />
    <property name="RootDirectoryCreator">
      <object id="CallHomeRootDirectoryCreator" type="ObjectCloud.CallHomePlugin.CallHomeRootDirectoryCreator, ObjectCloud.CallHomePlugin">
        <property name="FileHandlerFactoryLocator" ref="FileHandlerFactoryLocator" />
        <property name="DefaultRootPassword" value="root" />
      </object>
    </property>
    <property name="CallHomeFileHandlerFactory">
      <object id="CallHomeFileHandlerFactory" type="ObjectCloud.CallHomePlugin.CallHomeFileHandlerFactory, ObjectCloud.CallHomePlugin">
        <property name="FileHandlerFactoryLocator" ref="FileHandlerFactoryLocator" />
        <property name="DataAccessLocator">
          <object id="CallHomeDataAccessLocator" type="ObjectCloud.CallHomePlugin.DataAccessBase.DataAccessLocator, ObjectCloud.CallHomePlugin">
            <property name="DatabaseCreator">
              <object type="ObjectCloud.CallHomePlugin.DataAccess.EmbeddedDatabaseCreator, ObjectCloud.CallHomePlugin" id="CallHomePlugin.EmbeddedDatabaseCreator">
                <property name="EmbeddedDatabaseConnector" ref="EmbeddedDatabaseConnector" />
              </object>
            </property>
            <property name="DatabaseConnectorFactory">
              <object type="ObjectCloud.CallHomePlugin.DataAccess.DatabaseConnectorFactory, ObjectCloud.CallHomePlugin" id="CallHomePlugin.DatabaseConnectorFactory">
                <property name="EmbeddedDatabaseConnector" ref="EmbeddedDatabaseConnector" />
              </object>
            </property>
          </object>
        </property>
      </object>
    </property>
  </object>

</objects>