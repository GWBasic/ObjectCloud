<?xml version="1.0" encoding="utf-8" ?>
<ObjectCloudConfig>

  <FileSystem ConnectionString="./FileSystem" />
  <FileHandlerFactoryLocator Hostname="localhost"/>
	
  <WebServer
    Port="1082"
	  CacheSize="20000"
    MaxKnownMemoryUse="104857600"
    KeepAlive="true" />

  <!-- When running a real server, this line should be deleted.  Writing to the console really kills performance -->
  <LogHandlerFactory
	WriteToConsole="true"
	MaxChunkSize="5242880"
	MaxSize="524288000" />

</ObjectCloudConfig>